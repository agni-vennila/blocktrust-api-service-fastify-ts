generator client {
  provider = "prisma-client-js"
}

datasource db {
//  url      = env("DB_URI")
  provider = "sqlite"
  url      = "file:./dev.db"
//  provider = "postgresql"
//  url      = "postgres://postgres:postgrespw@localhost:5432/postgres"

}

model Category {
    id      Int     @id @default(autoincrement())
    name    String  @unique
}